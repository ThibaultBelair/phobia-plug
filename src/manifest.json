{
  "name": "Phobia Plug-in Dev",
  "options_page": "options.html",
  "permissions": [
    "webNavigation",
    "http://www.phobia.live/",
    "https://www.google.com/",
    "https://localhost:3001/fake.json"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "logo_transparent.png"
  },
  "icons": {
    "128": "logo_transparent.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["test.bundle.js"]
    }
  ]
}
